{
  "name": "livepack",
  "version": "0.4.0",
  "description": "Serialize live running code to Javascript",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./index.js"
    },
    "./register": {
      "import": "./es/register.js",
      "require": "./register.js"
    },
    "./babel": {
      "import": "./es/babel.js",
      "require": "./babel.js"
    },
    "./jest-transform": {
      "import": "./es/jest-transform.js",
      "require": "./jest-transform.js"
    }
  },
  "bin": "lib/cli.js",
  "files": [
    "babel.js",
    "register.js",
    "jest-transform.js",
    "lib/**/*.js",
    "es/**/*.js"
  ],
  "author": {
    "name": "Overlook Motel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overlookmotel/livepack.git"
  },
  "bugs": {
    "url": "https://github.com/overlookmotel/livepack/issues"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/generator": "^7.14.5",
    "@babel/helper-module-transforms": "^7.14.5",
    "@babel/parser": "^7.14.6",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@babel/traverse": "^7.14.5",
    "@babel/types": "^7.14.5",
    "babel-jest": "^27.0.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "base32.js": "^0.1.0",
    "convert-source-map": "^1.8.0",
    "find-up": "^5.0.0",
    "fs-extra": "^10.0.0",
    "is-it-type": "^5.0.0",
    "lodash": "^4.17.21",
    "native-type-of": "^1.0.0",
    "parse-node-version": "^1.0.1",
    "pirates": "^4.0.1",
    "reserved-words": "^0.1.2",
    "simple-invariant": "^2.0.1",
    "source-map": "^0.5.7",
    "string-splice": "^1.3.0",
    "strip-json-comments": "^3.1.1",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-arrow-functions": "^7.14.5",
    "@babel/plugin-transform-strict-mode": "^7.14.5",
    "@overlookmotel/eslint-config": "^9.0.4",
    "@overlookmotel/eslint-config-jest": "^5.0.1",
    "@overlookmotel/eslint-config-node": "^3.0.1",
    "eslint": "^7.29.0",
    "expect": "^27.0.2",
    "jest": "^27.0.4",
    "jest-expect-arguments": "^1.0.0",
    "jest-extended": "^0.11.5",
    "jest-matcher-utils": "^27.0.2",
    "mocha": "^9.0.1",
    "source-map-support": "^0.5.19"
  },
  "keywords": [
    "build",
    "live",
    "code",
    "package",
    "bundle"
  ],
  "scripts": {
    "test": "npm run lint && npm run test-main",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test-main": "npm run test-jest && npm run test-mocha",
    "test-jest": "jest; npm run test-cleanup",
    "test-mocha": "mocha ./test/*.mocha.test.js -R progress; npm run test-cleanup",
    "test-cleanup": "rm -rf test/_temp",
    "cover": "npm run cover-main && rm -rf coverage",
    "cover-main": "jest --coverage"
  },
  "engines": {
    "node": ">=12"
  },
  "readmeFilename": "README.md",
  "license": "MIT"
}
