{
  "name": "livepack",
  "version": "0.0.0",
  "description": "Serialize live running code to Javascript",
  "main": "index.js",
  "files": [
    "lib/**/*.js"
  ],
  "author": {
    "name": "Overlook Motel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overlookmotel/livepack.git"
  },
  "bugs": {
    "url": "https://github.com/overlookmotel/livepack/issues"
  },
  "dependencies": {
    "@babel/generator": "^7.10.3",
    "@babel/parser": "^7.10.3",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/register": "^7.10.3",
    "@babel/traverse": "^7.10.3",
    "@babel/types": "^7.10.3",
    "is-it-type": "^4.1.0",
    "lodash": "^4.17.15",
    "reserved-words": "^0.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@overlookmotel/eslint-config": "^7.2.1",
    "@overlookmotel/eslint-config-jest": "^4.1.1",
    "@overlookmotel/eslint-config-node": "^2.1.2",
    "babel-jest": "^26.1.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^26.1.0",
    "jest-expect-arguments": "^1.0.0",
    "jest-extended": "^0.11.5",
    "parse-node-version": "^1.0.1"
  },
  "keywords": [
    "build",
    "live",
    "code",
    "package",
    "bundle"
  ],
  "scripts": {
    "test": "npm run lint && npm run test-main",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test-main": "jest",
    "cover": "npm run cover-main && rm -rf coverage",
    "coveralls": "npm run cover-main && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cover-main": "cross-env COVERAGE=true jest --coverage",
    "ci": "if [ $COVERAGE ]; then npm run coveralls; else npm test; fi"
  },
  "engines": {
    "node": ">=10"
  },
  "readmeFilename": "README.md",
  "license": "MIT"
}
