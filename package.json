{
  "name": "livepack",
  "version": "0.7.3",
  "description": "Serialize live running code to Javascript",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./index.js"
    },
    "./register": {
      "import": "./es/register.js",
      "require": "./register.js"
    },
    "./instrument": {
      "import": "./es/instrument.js",
      "require": "./instrument.js"
    }
  },
  "bin": "lib/cli.js",
  "files": [
    "register.js",
    "instrument.js",
    "lib/**/*.js",
    "es/**/*.js"
  ],
  "author": {
    "name": "Overlook Motel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overlookmotel/livepack.git"
  },
  "bugs": {
    "url": "https://github.com/overlookmotel/livepack/issues"
  },
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@babel/generator": "^7.17.7",
    "@babel/helper-module-transforms": "^7.17.7",
    "@babel/parser": "^7.17.8",
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/traverse": "^7.17.3",
    "@babel/types": "^7.17.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "base32.js": "^0.1.0",
    "convert-source-map": "^1.8.0",
    "find-cache-dir": "^3.3.2",
    "find-up": "^5.0.0",
    "fs-extra": "^10.0.1",
    "is-it-type": "^5.1.2",
    "lodash": "^4.17.21",
    "native-type-of": "^1.1.0",
    "parse-node-version": "^2.0.0",
    "pirates": "^4.0.5",
    "reserved-words": "^0.1.2",
    "simple-invariant": "^2.0.1",
    "source-map": "^0.5.7",
    "source-map-support": "^0.5.21",
    "string-splice": "^1.3.0",
    "strip-json-comments": "^3.1.1",
    "yargs": "^17.4.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-arrow-functions": "^7.16.7",
    "@babel/plugin-transform-strict-mode": "^7.16.7",
    "@overlookmotel/eslint-config": "^10.0.3",
    "@overlookmotel/eslint-config-jest": "^6.0.1",
    "@overlookmotel/eslint-config-node": "^4.0.2",
    "eslint": "^8.12.0",
    "expect": "^27.5.1",
    "jest": "^27.5.1",
    "jest-expect-arguments": "^1.0.0",
    "jest-extended": "^2.0.0",
    "jest-matcher-utils": "^27.5.1",
    "mocha": "^9.2.2",
    "npm-run-all": "^4.1.5"
  },
  "keywords": [
    "build",
    "live",
    "code",
    "package",
    "bundle"
  ],
  "scripts": {
    "test": "npm run lint && npm run test-main",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test-main": "npm run test-jest && npm run test-mocha",
    "test-jest": "npm-run-all -c test-jest-main test-cleanup",
    "test-jest-main": "jest",
    "test-mocha": "npm-run-all -c test-mocha-main test-cleanup",
    "test-mocha-main": "mocha ./test/*.mocha.test.js -R progress",
    "test-cleanup": "rm -rf test/_temp",
    "cover": "npm-run-all -c cover-main cover-cleanup",
    "cover-main": "jest --coverage",
    "cover-cleanup": "rm -rf coverage"
  },
  "engines": {
    "node": ">=12"
  },
  "readmeFilename": "README.md",
  "license": "MIT"
}
